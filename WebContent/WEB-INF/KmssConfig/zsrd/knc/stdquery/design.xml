			ROLE_ZSRDKNCSTDQUERY_DEFAULT
		</roles>

		<request
			path="zsrd_knc_std_query_category/zsrdKncStdQueryCategory*"
			validatorParameter="recid=fdId,model=com.landray.kmss.zsrd.knc.stdquery.model.ZsrdKncStdQueryCategory">
		</request>

		<request
			path="zsrd_knc_std_query_detail/zsrdKncStdQueryDetail*"
