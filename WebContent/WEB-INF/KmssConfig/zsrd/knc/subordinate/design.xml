			ROLE_ZSRDKNCMARK_DEFAULT
		</roles>

		<request
			path="zsrd_knc_mark_detail/zsrdKncMarkDetail*"
			validatorParameter="recid=fdId,model=com.landray.kmss.zsrd.knc.mark.model.ZsrdKncMarkDetail">
		</request>

		<request
			path="zsrd_knc_mark_main/zsrdKncMarkMain*"
			validatorParameter="recid=fdId,model=com.landray.kmss.zsrd.knc.mark.model.ZsrdKncMarkMain">
		</request>

	</module>

	<homePage
		urlPrefix="zsrd/knc/mark"
		config="/moduleindex_notopic.jsp?nav=/zsrd/knc/mark/tree.jsp"
		url="/zsrd/knc/mark/index.jsp"
		messageKey="zsrd-knc-mark:module.zsrd.knc.mark" />
 

 
