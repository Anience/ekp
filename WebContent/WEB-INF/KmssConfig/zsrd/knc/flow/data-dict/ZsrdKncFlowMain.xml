			type="DateTime"
			column="fd_last_modified_time"
			notNull="false"
			validate="false" />

		<simpleProperty
			name="docPublishTime"
			messageKey="zsrd-knc-flow:zsrdKncFlowMain.docPublishTime"
			type="DateTime"
			column="doc_publish_time"
			notNull="false"
			validate="false" />

		<simpleProperty
			name="docCategoryId"
			messageKey="zsrd-knc-flow:zsrdKncFlowMain.docCategoryId"
			type="String"
			column="doc_category_id"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="fdVersion"
			messageKey="zsrd-knc-flow:zsrdKncFlowMain.fdVersion"
			type="String"
			column="fd_version"
			notNull="false"
			length="36"
			validate="false" />

		<simpleProperty
			name="fdSize"
			messageKey="zsrd-knc-flow:zsrdKncFlowMain.fdSize"
			type="String"
			column="fd_size"
			notNull="false"
			length="36"
			validate="false" />

		<modelProperty
			name="docCreator"
			messageKey="zsrd-knc-flow:zsrdKncFlowMain.docCreator"
			type="com.landray.kmss.sys.organization.model.SysOrgElement"
			column="doc_creator_id"
			notNull="false"
			validate="false" />

 

 
	</model>
</models>
