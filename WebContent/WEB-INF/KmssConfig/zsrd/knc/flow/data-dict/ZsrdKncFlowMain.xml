			messageKey="zsrd-knc-flow:zsrdKncFlowMain.docSubject"
			type="String"
			column="doc_subject"
			notNull="false"
			length="200"
			validate="false" />

		<simpleProperty
			name="fdDescription"
			messageKey="zsrd-knc-flow:zsrdKncFlowMain.fdDescription"
			type="String"
			column="fd_description"
			notNull="false"
			length="500"
			validate="false" />

		<simpleProperty
			name="docStatus"
			messageKey="zsrd-knc-flow:zsrdKncFlowMain.docStatus"
			type="String"
			column="doc_status"
			notNull="false"
			length="2"
			validate="false" />

		<simpleProperty
			name="docCreateTime"
			messageKey="zsrd-knc-flow:zsrdKncFlowMain.docCreateTime"
			type="DateTime"
			column="doc_create_time"
			notNull="false"
			validate="false" />

		<simpleProperty
