<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.knc.risk.model.ZsrdKncFlow"
		table="zsrd_knc_flow">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdName"
			column="fd_name"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdCode"
			column="fd_code"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdRisk"
			column="fd_risk"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />

		<property
			name="fdAlias"
			column="fd_alias"
			update="true"
			insert="true"
			length="50"
			not-null="false" />


	</class>
</hibernate-mapping>
