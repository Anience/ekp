<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.knc.risk.model.ZsrdKncRiskMain"
		table="zsrd_knc_risk_main">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdSubject"
			column="fd_subject"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdDescription"
			column="fd_description"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdLastModifiedTime"
			column="fd_last_modified_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docPublishTime"
			column="doc_publish_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdCode"
			column="fd_code"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdInfluence"
			column="fd_influence"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdPossibility"
			column="fd_possibility"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdGrade"
			column="fd_grade"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdControlFrequency"
			column="fd_control_frequency"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdControlType"
			column="fd_control_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdValue"
			column="fd_value"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdControlTarget"
			column="fd_control_target"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdControlStep"
			column="fd_control_step"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />

		<many-to-one
			name="docCategory"
			column="doc_category_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdType"
			column="fd_type"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdFlowLink"
			column="fd_flow_link_id"
			insert="true"
			update="true"
			not-null="false" />
			


	</class>
</hibernate-mapping>
