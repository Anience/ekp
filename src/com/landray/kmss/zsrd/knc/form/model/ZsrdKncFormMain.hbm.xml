<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.knc.form.model.ZsrdKncFormMain"
		table="zsrd_knc_form_main">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docSubject"
			column="doc_subject"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdDescription"
			column="fd_description"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdLastModifiedTime"
			column="fd_last_modified_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docPublishTime"
			column="doc_publish_time"
			update="true"
			insert="true"
			not-null="false" />


		<property
			name="fdVersion"
			column="fd_version"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdSize"
			column="fd_size"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
        <many-to-one
			name="docCategory"
			column="doc_category_id"
			insert="true"
			update="true"
			not-null="false" />

	</class>
</hibernate-mapping>
