<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.landray.kmss.zsrd.knc.form.model.ZsrdKncFormCategory"
		table="zsrd_knc_form_category">
		<id name="fdId" column="fd_id" length="36">
			<generator class="assigned" />
		</id>
		<property name="fdName" column="fd_name"
			update="true" insert="true" not-null="false"  />
		<property name="fdOrder" column="fd_order"
			update="true" insert="true" not-null="false"  />
		<property name="fdHierarchyId" column="fd_hierarchy_id"
			update="true" insert="true" not-null="false"  />
		<many-to-one name="hbmParent"
			class="com.landray.kmss.zsrd.knc.form.model.ZsrdKncFormCategory" column="fd_parent_id"
			insert="true" update="true" not-null="false" />

		<property name="authReaderFlag" column="auth_reader_flag"
			update="true" insert="true" not-null="false" length="1" />
		<property name="authAttNodownload" column="auth_att_nodownload"
			update="true" insert="true" not-null="false" length="1" />
		<property name="authRBPFlag" column="auth_rbp_flag" update="true"
			insert="true" />
		<property name="authAttNocopy" column="auth_att_nocopy"
			update="true" insert="true" not-null="false" length="1" />
		<property name="authAttNoprint" column="auth_att_noprint"
			update="true" insert="true" not-null="false" length="1" />
		<property name="authChangeAtt" column="auth_change_att"
			update="true" insert="true" />
		<bag name="authReaders" table="zsrd_knc_form_category_readers">
			<key column="fd_doc_id" />
			<many-to-many class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>
		<bag name="authEditors" table="zsrd_knc_form_category_editors">
			<key column="fd_doc_id" />
			<many-to-many class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>
		<bag name="authOtherReaders" table="zsrd_knc_form_category_oreader"
			lazy="true">
			<key column="fd_doc_id" />
			<many-to-many class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		<bag name="authOtherEditors" table="zsrd_knc_form_category_oeditor"
			lazy="true">
			<key column="fd_doc_id" />
			<many-to-many class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>
		<bag name="authAllReaders" table="zsrd_knc_form_category_areader"
			lazy="true">
			<key column="fd_doc_id" />
			<many-to-many class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		<bag name="authAllEditors" table="zsrd_knc_form_category_aeditor"
			lazy="true">
			<key column="fd_doc_id" />
			<many-to-many class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>
		<bag name="authAttDownloads" table="zsrd_knc_form_category_attdl"
			lazy="true">
			<key column="fd_doc_id" />
			<many-to-many class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_download_id" />
		</bag>
		<bag name="authAttCopys" table="zsrd_knc_form_category_attcopy"
			lazy="true">
			<key column="fd_doc_id" />
			<many-to-many class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_copy_id" />
		</bag>
		<bag name="authAttPrints" table="zsrd_knc_form_category_attprint"
			lazy="true">
			<key column="fd_doc_id" />
			<many-to-many class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_att_print_id" />
		</bag>
	</class>
</hibernate-mapping>
