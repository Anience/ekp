<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.knc.workofplan.model.ZsrdKncWorkOfPlan"
		table="zsrd_knc_work_of_plan">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdWorkName"
			column="fd_work_name"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
				name="fdWork"
				column="fd_work"
				update="true"
				insert="true"
				length="1000"
				not-null="false" />

		<property
			name="fdTime"
			column="fd_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdCycle"
			column="fd_cycle"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<many-to-one
			name="fdPerson"
			column="fd_person"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
