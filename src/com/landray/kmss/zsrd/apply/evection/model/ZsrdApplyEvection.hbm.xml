<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.evection.model.ZsrdApplyEvection"
		table="zsrd_apply_evection">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdToPlace"
			column="fd_to_place"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdTask"
			column="fd_task"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<property
			name="fdVehicle"
			column="fd_vehicle"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdEvectionStart"
			column="fd_evection_start"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdEvectionEnd"
			column="fd_evection_end"
			update="true"
			insert="true"
			length="1000"
			not-null="false" />
			
		<property
			name="fdPartnerIds"
			column="fd_partner_ids"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdDepart"
			column="fd_depart"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdPosition"
			column="fd_position"
			insert="true"
			update="true"
			not-null="false" />
			
