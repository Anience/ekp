<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.vacate.model.ZsrdApplyVacate"
		table="zsrd_apply_vacate">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdVacateReason"
			column="fd_vacate_reason"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<property
			name="fdVacateType"
			column="fd_vacate_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdVisit"
			column="fd_visit"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdVisitRelation"
			column="fd_visit_relation"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdVisitPlace"
			column="fd_visit_place"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<property
			name="fdVacateStart"
			column="fd_vacate_start"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdVacateEnd"
			column="fd_vacate_end"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdRegisterTime"
			column="fd_register_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="fdDepart"
			column="fd_depart"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdPosition"
			column="fd_position"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdVacatePerson"
			column="fd_vacateperson_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="fdAsk"
			column="fd_ask"
			update="true"
			insert="true"
			length="5"
			not-null="false" />
		
		<many-to-one
			name="fdOverPlus"
			column="fd_overplus_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<property
			name="fdVacateDate"
			column="fd_vacatedate"
			update="true"
			insert="true"
			length="10"
			not-null="false" />

	</class>
</hibernate-mapping>
