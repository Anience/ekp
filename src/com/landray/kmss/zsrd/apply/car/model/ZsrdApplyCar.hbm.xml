<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.car.model.ZsrdApplyCar"
		table="zsrd_apply_car">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdCarReason"
			column="fd_car_reason"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<property
			name="fdPhoneNum"
			column="fd_phone_num"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdRouteStart"
			column="fd_route_start"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdRouteBack"
			column="fd_route_back"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdUseTime"
			column="fd_use_time"
			update="true"
			insert="true"
			length="50"
			not-null="false" />

		<property
			name="fdCarNum"
			column="fd_car_num"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdRemark"
			column="fd_remark"
			update="true"
			insert="true"
			length="500"
			not-null="false" />
			
		<property
			name="fdDriver"
			column="fd_driver"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdTimeStart"
			column="fd_time_start"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<property
			name="fdUseCar"
			column="fd_use_car"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
		
		<property
			name="fdTimeBack"
			column="fd_time_back"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<many-to-one
			name="fdDepart"
			column="fd_depart"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
