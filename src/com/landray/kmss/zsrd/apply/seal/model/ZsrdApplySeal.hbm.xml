<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.seal.model.ZsrdApplySeal"
		table="zsrd_apply_seal">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdToCompany"
			column="fd_to_company"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdSealNum"
			column="fd_seal_num"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdRemark"
			column="fd_remark"
			update="true"
			insert="true"
			length="500"
			not-null="false" />
			
		<property
			name="fdSort"
			column="fd_sort"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<many-to-one
			name="fdServiceDepart"
			column="fd_service_depart"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdDepart"
			column="fd_depart"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<!-- 所有人可阅读标记 -->
		<property
			name="authReaderFlag"
			column="auth_reader_flag"
			update="true"
			insert="true"
			not-null="false"
			length="1" />
			
		<!-- 可阅读者 -->
		<bag
			name="authReaders"
			table="zsrd_apply_seal_readers">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag>
		
		<!-- 其他可阅读者 -->
		<bag
			name="authOtherReaders"
			table="zsrd_apply_seal_oreader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		
		<!-- 所有可阅读者 -->
		<bag
			name="authAllReaders"
			table="zsrd_apply_seal_areader"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_reader_id" />
		</bag>
		
		<!-- 可编辑者 -->
		<bag
			name="authEditors"
			table="zsrd_apply_seal_editors">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="fd_org_id" />
		</bag> 
		
		<!-- 其他可编辑者 -->
		<bag
			name="authOtherEditors"
			table="zsrd_apply_seal_oeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>
		
		<!-- 所有可编辑者 -->
		<bag
			name="authAllEditors"
			table="zsrd_apply_seal_aeditor"
			lazy="true">
			<key
				column="fd_doc_id" />
			<many-to-many
				class="com.landray.kmss.sys.organization.model.SysOrgElement"
				column="auth_other_editor_id" />
		</bag>
		
	</class>
</hibernate-mapping>
