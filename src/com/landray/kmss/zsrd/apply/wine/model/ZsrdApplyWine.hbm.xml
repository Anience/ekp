<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.wine.model.ZsrdApplyWine"
		table="zsrd_apply_wine">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdRemark"
			column="fd_remark"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<property
			name="fdReason"
			column="fd_reason"
			update="true"
			insert="true"
			length="400"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
				name="fdDepart"
				column="fd_depart"
				insert="true"
				update="true"
				not-null="false" />
			
		<property
			name="docCdadvice"
			column="doc_cdadvice"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<property
			name="docLeadergd"
			column="doc_leadergd"
			update="true"
			insert="true"
			length="500"
			not-null="false" />

		<list name="fdLinks" cascade="all-delete-orphan" lazy="true">
			<key column="fd_pid" />
			<list-index column="fd_order"></list-index>
			<one-to-many class="com.landray.kmss.zsrd.apply.wine.model.ZsrdApplyWinemg" />
		</list>

	</class>
</hibernate-mapping>
