<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.wine.model.ZsrdApplyWinemg"
		table="zsrd_apply_wineMg">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdTypename"
			column="fd_typename"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

		<property
			name="fdPrice"
			column="fd_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdNum"
			column="fd_num"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdPer"
			column="fd_per"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
					
		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="200"
			not-null="false" />

	    <property 
	        name="fdOrder" 
	        column="fd_order" 
	        update="true"
		    insert="true" 
		    not-null="false" />

		<many-to-one
			name="fd"
			column="fd_pid"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
