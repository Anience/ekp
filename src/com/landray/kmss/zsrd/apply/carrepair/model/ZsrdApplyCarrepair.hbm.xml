<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.carrepair.model.ZsrdApplyCarrepair"
		table="zsrd_apply_carrepair">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdCarnum"
			column="fd_carnum"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdReason"
			column="fd_reason"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />
			
		<property
			name="fdPrecost"
			column="fd_precost"
			update="true"
			insert="true"
			length="20"
			not-null="false" />	
	
		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
		

		<list name="fdProject" cascade="all-delete-orphan" lazy="true">
			<key column="fd_proid" />
			<list-index column="fd_order"></list-index>
			<one-to-many class="com.landray.kmss.zsrd.apply.carrepair.model.ZsrdApplyCarpro" />
		</list>


	</class>
</hibernate-mapping>
