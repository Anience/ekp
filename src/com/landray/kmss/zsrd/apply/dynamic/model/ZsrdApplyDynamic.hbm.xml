<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.dynamic.model.ZsrdApplyDynamic"
		table="zsrd_apply_dynamic">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="docSubject"
			column="doc_subject"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="docContent"
			column="doc_content"
			update="true"
			insert="true"
			length="1000000"
			not-null="false"
			type="com.landray.kmss.common.dao.ClobStringType"
			lazy="true" />

		<property
			name="fdDescription"
			column="fd_description"
			update="true"
			insert="true"
			length="1500"
			not-null="false" />

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="true" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="docAlterTime"
			column="doc_alter_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdLastModifiedTime"
			column="fd_last_modified_time"
			update="true"
			insert="true"
			not-null="true" />


		<property
			name="fdUrl"
			column="fd_url"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		
		<property
			name="docIsAnno"
			column="doc_isanno"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdDepart"
			column="fd_depart"
			insert="true"
			update="true"
			not-null="false" />
					
		<many-to-one
			name="docCategory"
			column="doc_category"
			insert="true"
			update="true"
			not-null="false" />
			
		<many-to-one
				name="fdCategory"
				column="fd_category"
				insert="true"
				update="true"
				not-null="false" />
	</class>
</hibernate-mapping>
