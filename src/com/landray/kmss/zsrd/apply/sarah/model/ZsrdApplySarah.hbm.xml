<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.sarah.model.ZsrdApplySarah"
		table="zsrd_apply_sarah">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdForeignDepartName"
			column="fd_foreign_depart_name"
			update="true"
			insert="true"
			length="300"
			not-null="false" />

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<property
			name="fdEndTime"
			column="fd_end_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdReceptionTime"
			column="fd_reception_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdPhoneNumber"
			column="fd_phone_number"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdReceptionNumber"
			column="fd_reception__number"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdBreakfast"
			column="fd_breakfast"
			update="true"
			insert="true"
			length="3"
			not-null="false" />

		<property
			name="fdLunch"
			column="fd_lunch"
			update="true"
			insert="true"
			length="3"
			not-null="false" />

		<property
			name="fdSupper"
			column="fd_supper"
			update="true"
			insert="true"
			length="3"
			not-null="false" />

		<property
			name="fdFoodNight"
			column="fd_food_night"
			update="true"
			insert="true"
			length="3"
			not-null="false" />

		<property
			name="fdGetTeary"
			column="fd_get_teary"
			update="true"
			insert="true"
			length="3"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

		<many-to-one
			name="fdDepart"
			column="fd_depart"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
