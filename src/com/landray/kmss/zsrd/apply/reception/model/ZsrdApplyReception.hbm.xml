<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.zsrd.apply.reception.model.ZsrdApplyReception"
		table="zsrd_apply_reception">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdForeignDepartName"
			column="fd_foreign_depart_name"
			update="true"
			insert="true"
			length="300"
			not-null="false" />

		<property
			name="docStatus"
			column="doc_status"
			update="true"
			insert="true"
			length="2"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdReceptionNum"
			column="fd_reception__num"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdAccompanyNum"
			column="fd_accompany_num"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdStd"
			column="fd_std"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdTotal"
			column="fd_total"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdReceptionType"
			column="fd_reception_type"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="fdReceptionAddress"
			column="fd_reception_address"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdReceptionTime"
			column="fd_reception_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
        <property
			name="fdRemark"
			column="fd_remark"
			update="true"
			insert="true"
			length="100"
			not-null="false" />

		<property
			name="fdAName"
			column="fd_a_name"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdAPrice"
			column="fd_a_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdANum"
			column="fd_a_num"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdBName"
			column="fd_b_name"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdBPrice"
			column="fd_b_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdBNum"
			column="fd_b_num"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
		<property
			name="docUseWine"
			column="doc_UseWine"
			update="true"
			insert="true"
			length="36"
			not-null="false" />
			
			
		<list name="fdLinks" cascade="all-delete-orphan" lazy="true">
			<key column="fd_pid" />
			<list-index column="fd_order"></list-index>
			<one-to-many class="com.landray.kmss.zsrd.apply.reception.model.ZsrdApplyReceptionmg" />
		</list>

	</class>
</hibernate-mapping>
